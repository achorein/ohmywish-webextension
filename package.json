{
  "name": "wishspace-webextension-vue",
  "version": "4.0.4",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-ff": "vue-cli-service build && mv dist/manifest.firefox.json dist/manifest.json && rm dist/manifest.*.json",
    "build-chrome": "vue-cli-service build && mv dist/manifest.chrome.json dist/manifest.json && rm dist/manifest.*.json",
    "build-opera": "vue-cli-service build && mv dist/manifest.chrome.json dist/manifest.json && rm dist/manifest.*.json",
    "build-edge": "vue-cli-service build && mv dist/manifest.edge.json dist/manifest.json && rm dist/manifest.*.json",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-regular-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/vue-fontawesome": "^0.1.2",
    "axios": "^0.27.2",
    "bootstrap": "^4.6.2",
    "bootstrap-vue": "^2.23.1",
    "core-js": "^3.8.3",
    "vue": "^2.7.15",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "node-sass": "^6.0.0",
    "sass-loader": "^10.0.0",
    "vue-template-compiler": "^2.7.15"
  },
  "resolutions": {
    "node-gyp": "^10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
